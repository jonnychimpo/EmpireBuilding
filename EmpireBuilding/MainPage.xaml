    								<TextBlock Grid.Column="2" HorizontalAlignment="Right" Text="{TemplateBinding Suffix}" VerticalAlignment="Center" FontFamily="/EmpireBuilding;component/Fonts/Fonts.zip#Buxton Sketch" Visibility="Collapsed"/>
