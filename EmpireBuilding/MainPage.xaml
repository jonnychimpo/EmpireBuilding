    								</Grid.ColumnDefinitions>
    								<TextBlock Grid.Column="0" HorizontalAlignment="Left" Text="{TemplateBinding Header}" VerticalAlignment="Center" TextAlignment="Right" FontFamily="/EmpireBuilding;component/Fonts/Fonts.zip#Buxton Sketch" Visibility="Collapsed"/>
