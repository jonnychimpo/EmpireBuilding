			<RowDefinition Height="Auto"/>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>

		<Canvas HorizontalAlignment="Left" Height="480" VerticalAlignment="Top" Width="853" Background="#FF3DA02D" Opacity="0.225" Visibility="Collapsed"/>
