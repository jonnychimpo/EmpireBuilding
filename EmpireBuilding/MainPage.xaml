			<Canvas x:Name="canvasGameBoard" Height="600" Width="680">
				<!--<ListBox x:Name="listBoxGameBoard" BorderThickness="1" Background="{x:Null}" ItemContainerStyle="{StaticResource GameboardStyle}" Foreground="#FFFFFFFE" Width="1520" ScrollViewer.VerticalScrollBarVisibility="Disabled" Visibility="Collapsed">
